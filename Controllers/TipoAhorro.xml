  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"TipoAhorro"."Id_TipoAhorro" "IDTipoahorro"
	,"TipoAhorro"."Descripcion" "Descripcion"
	,"TipoAhorro"."DescripcionBilingue" "DescripcionBilingue"
	,"TipoAhorro"."Numero" "Numero"
	,"TipoAhorro"."Texto" "Texto"
	,"TipoAhorro"."ID_Concepto" "IDConcepto"
	,"ID_Concepto"."Alias" "IDConceptoalias"
	,"ID_ConceptoID_TipoConcepto"."Descripcion" "IDConceptoidTipoconceptodescripcion"
	,"TipoAhorro"."ID_Concepto2" "IDConcepto2"
	,"ID_Concepto2"."Alias" "IDConcepto2alias"
	,"ID_Concepto2ID_TipoConcepto"."Descripcion" "IDConcepto2idTipoconceptodescripcion"
	,"TipoAhorro"."SaldarFiniquito" "SaldarFiniquito"
	,"TipoAhorro"."AutoAlta" "AutoAlta"
	,"TipoAhorro"."CreadoPor" "CreadoPor"
	,"TipoAhorro"."ModificadoPor" "ModificadoPor"
	,"TipoAhorro"."CreadoEn" "CreadoEn"
	,"TipoAhorro"."ModificadoEn" "ModificadoEn"
from "dbo"."TipoAhorro" "TipoAhorro"
	left join "dbo"."Concepto" "ID_Concepto" on "TipoAhorro"."ID_Concepto" = "ID_Concepto"."Id_Concepto"
	left join "dbo"."TipoConcepto" "ID_ConceptoID_TipoConcepto" on "ID_Concepto"."ID_TipoConcepto" = "ID_ConceptoID_TipoConcepto"."ID_TipoConcepto"
	left join "dbo"."Concepto" "ID_Concepto2" on "TipoAhorro"."ID_Concepto2" = "ID_Concepto2"."Id_Concepto"
	left join "dbo"."TipoConcepto" "ID_Concepto2ID_TipoConcepto" on "ID_Concepto2"."ID_TipoConcepto" = "ID_Concepto2ID_TipoConcepto"."ID_TipoConcepto"
]]></text>
    </command>
  </commands>
