  <businessRules>
    <rule id="r100" commandName="Calculate|Update" view="editForm1" type="Sql" phase="Execute" name="E_Contrasena"><![CDATA[declare @Success nvarchar (5) = 'true'
if (@PasswordPortal is not null and @ConfirmarPassword is not null and @ConfirmarContrasena is not null and @NuevoPassword is not null)
begin
    if (@PasswordPortal != @ConfirmarPassword)
	begin
		set @Result_Focus = 'ConfirmarPassword, Ingresa correctamente tu contraseña anterior por favor.'
    	set @Success = 'false'
	end
	if (@NuevoPassword != @ConfirmarContrasena)
	begin
		set @Result_Focus = 'ConfirmarContrasena, La confirmación de la nueva contraseña no coincide.'
    	set @Success = 'false'
	end
end
if (@Arguments_CommandName = 'Calculate' or @Success = 'false')
begin
    set @BusinessRules_PreventDefault = 1
end
if (@Success = 'true')
begin
	UPDATE Empleado
	SET PasswordPortal = @ConfirmarContrasena
	WHERE ID_Empleado = @IDEmpleado
end]]></rule>
  </businessRules>
